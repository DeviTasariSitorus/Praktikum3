import 'package:flutter/material.dart';

void main() {
  runApp(const MyApp());
}

class MyApp extends StatelessWidget {
  const MyApp({super.key});

  // This widget is the root of your application.
  @override
  Widget build(BuildContext context) {
    return MaterialApp(
      debugShowCheckedModeBanner: false,
      home: Devi_Tasari_Praktikum_3(),
    );
  }
}

class Devi_Tasari_Praktikum_3 extends StatelessWidget {
  const Devi_Tasari_Praktikum_3({Key? key}) : super (key: key);

  @override
  Widget build(BuildContext context) {
    return Scaffold(
    appBar: AppBar(
        title: Text('Devi_Tasari_Praktikum_3')
    ),
    body: Container(
      padding: EdgeInsets.all(50),
      child: Row(
        mainAxisAlignment: MainAxisAlignment.spaceEvenly,
        children: [
          Column(
            children: [
                Icon(Icons.access_alarm),
                Text("Alarm"),
        ],
    ),
          Column(
            children: [
                Icon(Icons.phone),
                Text("Phone"),
        ],
    ),
          Column(
            children: [
                Icon(Icons.book),
                Text("book"),
        ],
    ),
        ],
    ),
    ));
  }
}

